/* Agored */
/************************************************************************************************
 *
 * Variables
 *
 ***********************************************************************************************/
*,
*:before,
*:after {
  box-sizing: border-box; }

html,
button,
input,
select,
textarea {
  color: black; }

html {
  font-size: 100%;
  width: 100%;
  height: 100%;
  -webkit-font-smoothing: antialiased; }

::-moz-selection {
  background: black;
  color: white;
  text-shadow: none; }

::selection {
  background: black;
  color: white;
  text-shadow: none; }

hr {
  display: block;
  height: 1px;
  border: 0;
  border-top: 1px solid black;
  margin: 1em 0;
  padding: 0; }

img {
  vertical-align: middle;
  max-width: 100%;
  height: auto; }

main {
  display: block; }

fieldset {
  border: 0;
  margin: 0;
  padding: 0; }

a {
  color: blue;
  text-decoration: none;
  transition: color .3s ease-in-out; }
  a:hover {
    color: #0000b3;
    text-decoration: underline; }

nav ul,
nav ol {
  padding: 0; }

figure {
  margin: 2em 0; }

.wrapper {
  max-width: 62.8em;
  margin: 0 auto;
  position: relative;
  padding: 0 1em; }
  @media only screen and (min-width: 45em) {
    .wrapper {
      padding: 0 2em; } }

.global-wrapper {
  position: relative;
  z-index: 1;
  width: 100%;
  min-height: 100vh;
  display: flex;
  flex-direction: column; }

.main {
  flex: 1 0 auto;
  -ms-flex-preferred-size: auto; }

.site-header,
.site-footer {
  flex-shrink: 0; }

/**
 * Typographic styles
 *
 * This contains all basic typography rules and any @font-face imports.
 */
body {
  font-family: "Helvetica Neue", Arial, sans-serif;
  font-size: 14px;
  line-height: 1.5;
  color: black; }
  @media only screen and (min-width: 65em) {
    body {
      font-size: 17px; } }

h1, h2, h3, h4, h5, h6 {
  font-family: "Helvetica Neue", Arial, sans-serif;
  font-weight: 700;
  text-rendering: optimizeLegibility;
  margin: 0;
  line-height: 1.5; }

h5, .h5 {
  font-size: 1em; }

h6, .h6 {
  font-size: 1em; }

blockquote,
.quote {
  font-family: Georgia, serif;
  font-style: italic;
  text-indent: 0;
  margin-bottom: 1em;
  margin-left: 0; }

p {
  margin: 0 0 1.5;
  line-height: 1.5; }

.leading {
  font-size: 1.2em;
  line-height: 1.3em; }

::-webkit-input-placeholder {
  /* WebKit browsers */
  color: grey; }

:-moz-placeholder {
  /* Mozilla Firefox 4 to 18 */
  color: grey; }

::-moz-placeholder {
  /* Mozilla Firefox 19+ */
  color: grey; }

:-ms-input-placeholder {
  /* Internet Explorer 10+ */
  color: grey; }

.form {
  margin-bottom: 1em; }
  .form__row {
    margin-bottom: 1em;
    overflow: visible; }
  .form__label {
    display: block;
    margin-bottom: 0.5em;
    font-size: 1em;
    color: black; }
    .form__row--error .form__label {
      color: #ff0000; }
  .form__field {
    background-color: white;
    box-shadow: none;
    border: 1px solid black;
    border-radius: 2px;
    padding: 0.8em 1em;
    font-size: 1em;
    font-weight: normal;
    width: 100%;
    transition: all 300ms ease-in-out; }
    .form__field:hover, .form__field:focus {
      outline: none; }
    .form__field--textarea {
      resize: vertical; }
    .form__row--error .form__field,
    .form__row--error .form__field:focus {
      box-shadow: inset 0 0 0 1px #ff0000;
      border-color: #ff0000; }

.chkrad__field {
  display: inline-block;
  margin-right: 1em; }
  .chkrad__field--error {
    border: 0;
    box-shadow: none; }

.chkrad__label {
  display: inline-block; }

.errors-list {
  list-style-type: none;
  color: #ff0000;
  font-size: 0.9em;
  padding: 0;
  margin: 0; }

.custom-select {
  padding: 0;
  position: relative; }
  .custom-select:after {
    content: "";
    position: absolute;
    width: 2em;
    height: 2em;
    top: 0;
    bottom: 0;
    margin: auto 0;
    right: 1em;
    background-size: 100% auto;
    z-index: 2;
    pointer-events: none; }
  .custom-select__field {
    background: none repeat scroll 0% 0% transparent;
    border: 1px solid transparent;
    outline: medium none;
    width: 100%;
    width: calc(100% + 3em);
    padding: 0.8em 1em;
    margin: 0;
    -webkit-appearance: none; }
    .custom-select__field:focus {
      width: 100%;
      outline: none;
      border: 1px solid black; }

